<% provide(:title, @user.name) %>
<div class="container homemain">
  <h1 id="usermid">
    <%= gravatar_for @user %> 
    <%= @user.name %>
  </h1>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <!-- If user is a service display menu -->
      <% if @user.service %>
        <% if @menu %>
          <%= render @menu %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>