<% provide(:title, "Edit Menu") %>
<div class="container homemain">
  <h1>Edit Menu</h1>
  <div class="row">
    <div class="col-md-6 col-md-offset-3 float-left">
      <%= form_for(@menu) do |f| %>

        <%= f.label :description %>
        <%= f.text_field :description, class:"form-control" %>

        <%= f.submit "Edit My Menu", class:"signupbutt btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>